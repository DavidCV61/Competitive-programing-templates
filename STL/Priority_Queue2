#include <bits/stdc++.h>
using namespace std;
  

// Driver Code
int main()
{
    priority_queue<int, vector<int>, greater<int> > pq;
    pq.push(10);
    pq.push(30);
   pq.push(20);
    pq.push(5);
    pq.push(1);
    cout << "\nSize : " << pq.size();
    cout << "\nTop : " << pq.top();
  
    cout << "\nThe priority queue pq is : "<<endl;

    while(!pq.empty()){

    	cout<<pq.top()<<endl;
    	pq.pop();
    }
    
   
    
  
    return 0;
}
